    "import time\n",
    "def tic():\n",
    "    tic.start = time.perf_counter()\n",
    "\n",
    "def toc():\n",
    "    elapsed_seconds = time.perf_counter() - tic.start\n",
    "    return elapsed_seconds # fractional"
